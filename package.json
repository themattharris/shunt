{
  "name": "shunt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "NODE_ENV=production next build",
    "start": "next start",
    "eslint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --max-warnings=${ESLINT_MAX_WARNINGS:-0}",
    "prettier": "prettier --write \"**/*.ts*\" \"**/*.js*\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:codecov": "vitest run --coverage",
    "predeploy": "pnpm prettier && pnpm build && pnpm eslint && pnpm typecheck && pnpm test:codecov",
    "postinstall": "prisma generate",
    "package:bump": "ncu -t minor -u && pnpm install",
    "prisma:migrate:prod": "dotenv -e .vercel/.env.production.local -- prisma migrate deploy"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.19.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/typography": "^0.5.19",
    "@vercel/analytics": "^1.6.1",
    "@vercel/kv": "^3.0.0",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "culori": "^4.0.2",
    "d3-geo": "^3.1.1",
    "date-fns": "^4.1.0",
    "geoip-lite": "^1.4.10",
    "geojson": "^0.5.0",
    "lodash": "^4.17.23",
    "lucide-react": "^0.562.0",
    "next": "15.5.9",
    "next-themes": "^0.4.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-simple-maps": "^3.0.0",
    "recharts": "^2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "topojson-client": "^3.1.0",
    "tw-animate-css": "^1.4.0",
    "ua-parser-js": "^2.0.8",
    "user-agents": "^1.1.669"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^9.9.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/culori": "^2.1.1",
    "@types/d3-geo": "^3.1.0",
    "@types/date-fns": "^2.6.3",
    "@types/geoip-lite": "^1.4.4",
    "@types/geojson": "^7946.0.16",
    "@types/lodash": "^4.17.23",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-simple-maps": "^3.0.6",
    "@types/recharts": "^1.8.29",
    "@types/topojson-client": "^3.1.5",
    "@types/user-agents": "^1.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.9",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.2",
    "prettier": "^3.8.1",
    "prisma": "^6.19.2",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,mjs,js,json,yml,yaml}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "packageManager": "pnpm@10.28.1"
}
